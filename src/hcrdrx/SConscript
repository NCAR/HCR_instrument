# -*- python -*-

tools = Split("""
profilerdds
pentek
qtt_qtconfig
boost_date_time
boost_program_options
doxygen
""")

sources = Split("""
main.cpp
""")

headers = Split("""
""")

env = Environment(tools = ['default'] + tools)

profilerdrx = env.Program('profilerdrx', sources)

html = env.Apidocs(sources + headers, DOXYFILE_FILE = "Doxyfile")

Default(profilerdrx, html)

