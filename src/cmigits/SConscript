# -*- python -*-
#
# Build cmigitsDaemon
#

tools = ['cmigits', 'qt4', 'xmlrpc_server_abyss++', 'rapradar']

env = Environment(tools = ['default'] + tools)
env.EnableQt4Modules(['QtCore', 'QtGui'])

daemon = env.Program('cmigitsDaemon', 'cmigitsDaemon.cpp')
Default(daemon)

playback = env.Program('Ts2CmigitsShm', 'Ts2CmigitsShm.cpp')
Default(playback)